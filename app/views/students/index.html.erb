<p id="notice"><%= notice %></p>

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.first_name + " " + student.last_name %></td>
        <td><%= link_to 'Add to section', new_student_section_student_path(student.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
